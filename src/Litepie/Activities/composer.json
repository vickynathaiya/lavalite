{
    "name": "litepie/laravel-activitylog",
    "description": "A very simple activity logger to monitor the users of your website or application",
    "homepage": "https://github.com/litepie/activities",
    "keywords":
    [
        "lavalite",
        "log",
        "user",
        "activity",
        "laravel"
    ],
    "authors": [
        {
            "name": "Renfos Technologies Pvt. Ltd.",
            "email": "info@renfos.com"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0",
        "spatie/string": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Lavalite\\Activities\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lavalite\\Activities\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "MIT",
    "extra": {
        "laravel": {
            "providers": [
                "Lavalite\\Activities\\ActivitiesServiceProvider"
            ]
        }
    }
}
